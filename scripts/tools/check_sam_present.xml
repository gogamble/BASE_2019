<tool id="check_sam_present" name="Check the present sam" version="0.1.0">
    <description>check_sam_present</description>
    <requirements>
    </requirements>
    <command detect_errors="exit_code" interpreter="python">check_sam_present.py
--fq=$fq
--alnType=$alnType
--samPath=$samPath
--G1=$G1
--G2=$G2
--out=$out
    </command>
    <inputs>
        <param name="fq" type="data" format="tabular" label="FastQ file" help=""/>
        <param name="alnType" size="30" type="select" value="" display="radio" label="Select single end or paired end alignments" help="">
            <option value="SE" selected="true">Single end</option>
            <option value="PE" selected="true">Paired end</option>
        </param>
        <param name="samPath" type="data" format="tabular" label="SAM file" help=""/> 
        <param name="G1" type="text" label="Tester" help=""/>
        <param name="G2" type="text" label="Line" help=""/>
    </inputs>
    <outputs>
        <data format="tabular" name="out" label="${tool.name} on ${on_string}:Check info"/>
    </outputs>
    <help><![CDATA[
        TODO: Fill in help.
    ]]></help>
</tool>
