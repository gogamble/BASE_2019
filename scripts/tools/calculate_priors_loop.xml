<tool id="calculate_priors_loop" name="Calculate priors loop" version="0.1.0">
    <description>calculate_priors_loop</description>
    <macros>
        <import>macros.xml</import>
    </macros>
    <requirements>
    </requirements>
    <command detect_errors="exit_code" interpreter="python">calculate_priors_loop.py
--output=$output
--g1=$g1
--g2=$g2
--sam-compare-dir=$sam-compare-dir
    </command>
    <inputs>
        <param name="sam-compare-dir" type="data" format="tabular" label="Sam compare output" help=""/>
        <param name="g1" type="text" label="column with G1 values" help=""/>
        <param name="g2" type="text" label="column with G2 values" help=""/>
    </inputs>
    <outputs>
        <data format="tabular" name="output" label="${tool.name} on ${on_string}:Check info"/>
    </outputs>
    <help><![CDATA[
        TODO: Fill in help.
    ]]></help>
</tool>


