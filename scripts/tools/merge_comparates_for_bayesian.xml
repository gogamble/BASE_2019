<tool id="BASE_merge_comparates_for_bayesian" name="Merge comparates for bayesian" version="0.1.0">
    <description>merge_comparates_for_bayesian</description>
    <command detect_errors="exit_code" interpreter="python">merge_comparates_for_bayesian_03amm.py
--output=$output
--comp=$comp
--design=$design
--begin=$begin
--end=$end
    </command>
    <inputs>
        <param name="comp" type="data" format="tabular" label="Summed Counts tables" help="Input filtered/summarized count tables per one comparate"/>
        <param name="design" type="data" format="tabular" label="Design file" help=""/>
        <param name="begin" type="text" label="Begin" help="Design file subset beginning(row number)"/>
        <param name="end" type="text" label="End" help="Design file subset end (row number)"/>
    </inputs>
    <outputs>
        <data name="output" format="tabular" label="${tool.name} on ${on_string}:Merge Comparates"/>
    </outputs>
    <help><![CDATA[
        TODO: Fill in help.
    ]]></help>
</tool>
