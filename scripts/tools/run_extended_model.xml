<tool id="run_extended_model" name="Run extended bayesian model" version="0.1.0">
    <description>run_extended_model</description>
    <macros>
        <import>macros.xml</import>
    </macros>
    <requirements>
    </requirements>
    <command detect_errors="exit_code" interpreter="python">run_extended_model.py
--datafile=$datafile
--subpath=$subpath
--compid=$compid
--cond=$cond
--workdir=$workdir
--output=$output
    </command>
    <inputs>
        <param name="datafile" type="data" format="tabular" label="Input File" help=""/>
        <param name="subpath" type="" label="" help=""/>
    </inputs>
    <outputs>
        <data format="tabular" name="compid" label="${tool.name} on ${on_string}:Check info"/>
        <data format="tabular" name="cond" label="${tool.name} on ${on_string}:Check info"/>
        <data format="tabular" name="workdir" label="${tool.name} on ${on_string}:Check info"/>
        <data format="tabular" name="output" label="${tool.name} on ${on_string}:Check info"/>
    </outputs>
    <help><![CDATA[
        TODO: Fill in help.
    ]]></help>
</tool>


